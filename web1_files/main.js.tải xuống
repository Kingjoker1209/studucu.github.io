/*
	Name: Theme Landing Page
	Version: 4.0
	Author: pixelwars
*/

(function($) { "use strict"; 

	
	// ------------------------------------------------------------
	// DOCUMENT READY
	$(function() {
		


		// open links in new window - fails to load when in iframe
		if ( top.location !== self.location ) {
			
			$('.button.primary').each( function() {
				$(this).attr("target","_blank");
			});
			
			}
		
		
		// ------------------------------
		// AOS shortcut - works on parent element attribute
		$('.aos').each(function() {
			
			var animation = $(this).data('animation');
			$(this).children().each(function(index) {
				$(this).attr('data-aos', animation );
				$(this).attr('data-aos-delay', ((index%12)+1)*50 );
			});
				
		});
		
		

	  
	  	// ------------------------------
		// SLIDER
		var slider = $('.slider');
		
		// if slider exists
		if(slider.length) {
			
			// support multiple sliders on the same page
			slider.each(function(index, element) {
				
				var items = $(element).data('items');
				var center = $(element).data('center');
				var arrows = $(element).data('arrows') ==! null ? true : false;
				
				if(center && items === 1) {
					$(element).addClass('center');
				}
		
				$(element).on('setPosition', function(event, slick, direction){
					
					// fix refresh AOS after slider loaded
					refresh_aos();
					
				}).slick({
					dots: $(element).data('dots'),
					arrows: arrows,
  					speed: 300,
					//fade: true,
					centerMode: center,
					infinite: true,
					centerPadding: items === 1 ? '25%' : '10%',
					slidesToShow: items,
					accessibility: true,
					
					responsive: [
						{
							breakpoint: 1400,
							settings: {
								slidesToShow: (items > 1) ? 2 : 1,
								centerMode: true,
								centerPadding: '10%'
							}
						},
						{
							breakpoint: 780,
							settings: {
								slidesToShow: 1,
								centerMode: true,
								centerPadding: '10%'
							}
						}
					]
				
				}) // end : slick()
				
				.on('click', '.slick-slide:not(.slick-current)', function (e) { // go to slide when clicked inactive slide : used in center mode
				
					if(center && items === 1) {
						e.stopPropagation();
						var index = $(this).data("slick-index");
						if ($(element).slick('slickCurrentSlide') !== index) {
						  $(element).slick('slickGoTo', index);
						}
						return false;
					}
					
				});  // end : on click
				
				
                
            }); // end: each loop
		
		} // if slider exists
		// ------------------------------
		  
		
		
		
		// ------------------------------
		// Scroll to Hash
		var sectionMargin = $('body').hasClass('is-header-sticky') ? 60 : 0;
		$(document).on('click', '.header a[href^="#"], a.button[href^="#"]', function (event) {
			event.preventDefault();
		
			$('html, body').stop().animate({
				scrollTop: $($.attr(this, 'href')).offset().top - sectionMargin
			}, 800, "easeInOutExpo");
		});
		// ------------------------------
		
		
		// ------------------------------
		// Back To Top
		if ($('#back-to-top').length) {
			var scrollTrigger = $(window).height() - 400, // px
				backToTop = function () {
					var scrollTop = $(window).scrollTop();
					if (scrollTop > scrollTrigger) {
						$('#back-to-top').addClass('show');
					} else {
						$('#back-to-top').removeClass('show');
					}
				};
			backToTop();
			$(window).on('scroll', function () {
				backToTop();
			});
			$('#back-to-top').on('click', function (e) {
				e.preventDefault();
				$('html,body').stop().animate({
					scrollTop: 0
				}, 700, 'easeInOutExpo');
			});
		}
		// ------------------------------
		
		
		
		
		
		
		// ------------------------------
		// POST THUMBNAILS
        var post_thumbnail = $('.has-bg');
		if(post_thumbnail.length) {

			
			// optimized background images for various screens
		  	post_thumbnail.each(function() {
				
				var postThumbnail = $(this);
				var src = "";
				
				if (postThumbnail.width() * window.devicePixelRatio > 1060) { // big screens
					src = postThumbnail.data('large-image');
				} else if (postThumbnail.width() * window.devicePixelRatio > 550) { // retina phones or tablets
					src = postThumbnail.data('medium-image');
				}
				
				if(src !== "" && src !== undefined ) {
				
					var tmpImg = new Image() ;
					tmpImg.src = src;
					tmpImg.onload = function() {
						postThumbnail.css('background-image', 'url(' + src + ')');
					};
					
				}
				
            });
		
        }
		// ------------------------------
		
		
		
		
		
		// ------------------------------
		// STICKY: HEADER
		window.addEventListener('scroll', function(){
			var distanceY = window.pageYOffset || document.documentElement.scrollTop,
				shrinkOn = 100,
				header = $('.is-header-sticky .header');
			if (distanceY > shrinkOn) {
				header.addClass("stuck");
			} else {
				if (header.hasClass("stuck")) {
					header.removeClass("stuck");
				}
			}
		});
		// ------------------------------
		
		
		
		
		// ------------------------------
		// ANIMATED GRID
		if(document.getElementById( 'grid' )){
			new AnimOnScroll( document.getElementById( 'grid' ), {
					minDuration : 0.3,
					maxDuration : 0.9,
					viewportFactor : 0.3
				} );
			
		}
		// ------------------------------
		
		
		
		// ------------------------------
		// ENVATO ELEMENTS CHECK
		if(getUrlParameter('storefront') === "envato-elements") {
			$('.hide-on-elements').hide();
			$('.show-on-elements').show();
		}
		// ------------------------------
		
		
		// ------------------------------
		// AOS : animate on scroll
		aos();
		// ------------------------------
		
		
		
		
		setupLightbox();
		
		
		
    });
    // DOCUMENT READY
	// ------------------------------------------------------------



	// ------------------------------------------------------------
	// WINDOW ONLOAD
	window.onload = function() {
		
		// fix : refresh AOS after all loaded
		refresh_aos();
			
	};
	// WINDOW ONLOAD	
	// ------------------------------------------------------------
	
	
	
	// ------------------------------------------------------------
	// FUNCTIONS
	
	
	// ------------------------------
	// AOS : animate on scroll
	function aos() {
			AOS.init({
			  offset: 200,
			  duration: 500,
			  easing: 'ease-out-quart',
			  delay: 100,
			  once: true
			});
		}
	function refresh_aos() {
			setTimeout(function(){ AOS.refresh(); },500);
		}
	// ------------------------------
	
	
	// ------------------------------
	// FN : GET URL PARAMETER - to check envato elements
	function getUrlParameter(name) {
		name = name.replace(/[\[]/, '\\[').replace(/[\]]/, '\\]');
		var regex = new RegExp('[\\?&]' + name + '=([^&#]*)');
		var results = regex.exec(location.search);
		return results === null ? '' : decodeURIComponent(results[1].replace(/\+/g, ' '));
	}
	// ------------------------------
			  
    
	
	// ------------------------------
	// LIGHTBOX - applied to porfolio and gallery post format
	function setupLightbox() {	
		
		//$('.video-popup').magnificPopup({type:'iframe'});
		
		if($(".video-popup").length) {
			
			$('.video-popup').each(function(index, element) {
				var $media_box = $(this);
				$media_box.magnificPopup({
				  type:'iframe',
				  iframe: {
					 markup: '<div class="mfp-iframe-scaler">'+
								'<div class="mfp-close"></div>'+
								'<div class="mfp-title">Some caption</div>'+
								'<iframe class="mfp-iframe" frameborder="0" allowfullscreen></iframe>'+
							  '</div>'
				  },
				  mainClass: 'mfp-zoom-in',
				  tLoading: '',
				  removalDelay: 300, //delay removal by X to allow out-animation
				  callbacks: {
					markupParse: function(template, values, item) {
						  var title = item.el.data('title') == undefined ? "" : item.el.data('title');
					 		values.title = title;
							  
					},
					imageLoadComplete: function() {
					  var self = this;
					  setTimeout(function() {
						self.wrap.addClass('mfp-image-loaded');
					  }, 16);
					},
					close: function() {
					  this.wrap.removeClass('mfp-image-loaded');
					},
					beforeAppend: function() {
						
						var self = this;
						
						
						this.content.find('iframe').on('load', function() {
						  setTimeout(function() {
							self.wrap.addClass('mfp-image-loaded');
						  }, 16);
						});
						
					 }
				  },
				  closeBtnInside: false,
				  closeOnContentClick: true,
				  midClick: true
				});
			});	
		}
		
	}
	// ------------------------------
	
	
	// ------------------------------------------------------------
	

})(jQuery);



// ------------------------------------------------------------
// PLUGINS


/*! modernizr 3.6.0 (Custom Build) | MIT *
